{

  "Jwt": {
    "Issuer": "your_issuer",
    "Audience": "your_audience"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Postgres_Users": "Host=localhost;Port=5432;Database=Game03_Users;Username=postgres;Password=",
    "Postgres_Data": "Host=localhost;Port=5432;Database=Game03_Data;Username=postgres;Password="
  },
  
  "MongoDb": {
    "ConnectionString": "mongodb://localhost:27017",
    "DataBases": [ // DataBases массив

      {
        "Name": "userData", // Имя базы данных внутри объекта массива
        "Collections": [ // Collections массив
          {
            "Name": "heroes" // Имя коллекции
          },
          {
            "Name": "equipment"
          }
        ]
      },

      // другие базы данных
      {
        "Name": "OtherDb",
        "Collections": [
          {
            "Name": "Logs"
          }
        ]
      }

    ]
  },
  
  "WebSocketSettings": {
    "Url": "wss://localhost:7227/ws/", // Изменено на WSS и порт 7227
    "ServerUrl": "https://localhost:7227/ws/", // Изменено на HTTPS и порт 7227
    "MaxConnections": 10000,
    "ReceiveBufferSize": 8192,
    "CloseTimeoutMs": 100,
    "CleanupBatchSize": 50,
    "MaxParallelCleanup": 10
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning",
        // Добавьте или измените следующие строки, чтобы увидеть привязку Kestrel
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.Server.Kestrel": "Information"
      }
    }
  }
}