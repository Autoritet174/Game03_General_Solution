<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
	<LangVersion>13.0</LangVersion>
	<GenerateDocumentationFile>False</GenerateDocumentationFile>
	<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>
<PropertyGroup>
  <GenerateDocumentationFile>true</GenerateDocumentationFile>
  <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
</PropertyGroup>
  <ItemGroup>
    <Compile Remove="DataBaseModels\**" />
    <Compile Remove="publish\**" />
    <Content Remove="DataBaseModels\**" />
    <Content Remove="publish\**" />
    <EmbeddedResource Remove="DataBaseModels\**" />
    <EmbeddedResource Remove="publish\**" />
    <None Remove="DataBaseModels\**" />
    <None Remove="publish\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Server.csproj.Backup.tmp" />
  </ItemGroup>

  <ItemGroup>
    <None Include="ClientManager2.cs" />
    <None Include="stylecop.json" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Authentication" Version="2.3.0" />
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="9.0.10" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="9.0.10">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="8.14.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\General\General.csproj" />
    <ProjectReference Include="..\Server.DB.Data\Server.DB.Data.csproj" />
    <ProjectReference Include="..\Server.DB.UserData\Server.DB.UserData.csproj" />
    <ProjectReference Include="..\Server.DB.Users\Server.DB.Users.csproj" />
  </ItemGroup>

</Project>
